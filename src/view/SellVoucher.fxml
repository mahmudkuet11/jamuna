<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="462.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SellVoucherController">
   <children>
      <Label layoutX="31.0" layoutY="29.0" text="Sell Voucher">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <Separator layoutX="21.0" layoutY="63.0" prefHeight="0.0" prefWidth="562.0" />
      <DatePicker fx:id="date" layoutX="401.0" layoutY="75.0" promptText="Select date" />
      <ComboBox fx:id="select_customer" layoutX="31.0" layoutY="75.0" onAction="#onCusomerSelect" prefHeight="25.0" prefWidth="229.0" promptText="Select Customer" />
      <TextField fx:id="name" layoutX="31.0" layoutY="111.0" prefHeight="25.0" prefWidth="229.0" promptText="Customer Name" />
      <TextField fx:id="phone" layoutX="31.0" layoutY="142.0" prefHeight="25.0" prefWidth="229.0" promptText="Phone" />
      <TextArea fx:id="address" layoutX="33.0" layoutY="174.0" prefHeight="54.0" prefWidth="229.0" promptText="Address" />
      <Separator layoutX="21.0" layoutY="237.0" prefHeight="0.0" prefWidth="562.0" />
      <Label layoutX="33.0" layoutY="250.0" text="Select Category" />
      <ComboBox fx:id="category" layoutX="129.0" layoutY="246.0" onAction="#onCategorySelect" prefHeight="25.0" prefWidth="174.0" />
      <Label layoutX="332.0" layoutY="250.0" text="Select Model" />
      <ComboBox fx:id="model" layoutX="412.0" layoutY="246.0" onAction="#onModelSelect" prefHeight="25.0" prefWidth="174.0" />
      <Label layoutX="33.0" layoutY="283.0" text="Select Serial No" />
      <ComboBox fx:id="sl_no" layoutX="129.0" layoutY="279.0" onAction="#onSerialSelect" prefHeight="25.0" prefWidth="174.0" />
      <Separator layoutX="21.0" layoutY="320.0" prefHeight="3.0" prefWidth="562.0" />
      <Label layoutX="33.0" layoutY="335.0" text="Price" />
      <TextField fx:id="price" layoutX="129.0" layoutY="329.0" onKeyReleased="#onEnterPrice" prefHeight="25.0" prefWidth="174.0" />
      <Label layoutX="33.0" layoutY="370.0" text="In Words" />
      <TextField fx:id="price_word" layoutX="129.0" layoutY="366.0" prefHeight="25.0" prefWidth="450.0" />
      <Button fx:id="reset" layoutX="172.0" layoutY="412.0" mnemonicParsing="false" onAction="#onResetButtonClick" prefHeight="25.0" prefWidth="83.0" text="Reset" />
      <Button fx:id="ok" layoutX="261.0" layoutY="412.0" mnemonicParsing="false" onAction="#onOkButtonClick" prefHeight="25.0" prefWidth="83.0" text="OK" />
      <Button fx:id="cancel" layoutX="352.0" layoutY="412.0" mnemonicParsing="false" onAction="#onCancelButtonClick" prefHeight="25.0" prefWidth="83.0" text="Cancel" />
   </children>
</AnchorPane>
